{
  "name": "@mrbuilder/example-app-svelte",
  "description": "This shows how to use svelte plugin",
  "version": "4.3.0",
  "private": true,
  "dependencies": {
  },
  "devDependencies": {
    "@mrbuilder/preset-app": "^4.3.0",
    "@mrbuilder/plugin-svelte": "^4.3.0"
  },
  "scripts": {
    "start": "mrbuilder",
    "prepare": "mrbuilder --app build",
    "test": "mrbuilder"
  },
  "mrbuilder": {
    "plugins": [
      [
        "@mrbuilder/plugin-react",
        false
      ],
      "@mrbuilder/plugin-svelte",
      [
        "@mrbuilder/plugin-html",
        {
          "template": "./public/index.html"
        }
      ]
    ],
    "presets": [
      "@mrbuilder/preset-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
