{
  "name": "@mrbuilder/plugin-css",
  "version": "4.3.0-alpha.0",
  "description": "CSS support for mbuilder",
  "dependencies": {
    "@mrbuilder/plugin-browserslist": "^4.3.0-alpha.0",
    "autoprefixer": "^9.7.5",
    "css-loader": "^3.4.2",
    "find-package": "^1.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.2",
    "style-loader": "^1.1.3"
  },
  "scripts": {},
  "devDependencies": {
    "@mrbuilder/plugin-mocha": "^4.3.0-alpha.0"
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "main": "src",
  "source": "src",
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "test": "/\\.css$/",
      "publicPath": "/public",
      "modules": "/\\.cssm$/",
      "sideEffects": true
    }
