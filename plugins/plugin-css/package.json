  "description": "CSS support for mbuilder",
  "dependencies": {
    "@mrbuilder/plugin-browserslist": "^4.3.7",
    "autoprefixer": "^10.2.1",
    "css-loader": "^3.6.0",
    "find-package": "^1.0.0",
    "mini-css-extract-plugin": "^1.3.4",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^5.0.2",
    "style-loader": "^1.2.1"
  },
  "scripts": {},
  "devDependencies": {
    "@mrbuilder/plugin-mocha": "^4.3.7"
  },
