{
  "name": "@mrbuilder/plugin-graphql",
  "version": "4.1.0",
  "description": "Loading graphql webpack",
  "dependencies": {
    "graphql-tag": "^2.9.2"
  },
  "devDependencies": {
    "@mrbuilder/plugin-mocha": "^4.1.0"
  },
  "source": "src",
  "main": "src",
  "license": "0BSD",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "gitHead": "8932fa60b6c6f6ed18b45918e7873c8e70b8d0be",
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "test": "/\\.(?:graphql|gql)s?$/",
      "loader": "graphql-tag/loader",
      "exclude": "/node_modules/"
    }
  }
}
